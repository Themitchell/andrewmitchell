<!DOCTYPE html>
<html>
<head>
  <title>Blog</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div id="container">
		
		<div id="header">
			<div id="logo"><h4>Test</h4></div>
			<%= render :partial => 'shared/login_nav' %>
		</div>
		
		
		<%= render :partial => 'shared/nav' %>
		
		
		<div id="content">
			<div id="main">
				<%= yield %>
			</div>
			
			<%= render :partial => 'shared/side' %>
		</div>
		
		
		<%= render :partial => 'shared/footer' %>
	</div>
</body>
</html>
